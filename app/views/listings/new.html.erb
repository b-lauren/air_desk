<h1>New Listing</h1>

<%= simple_form_for(@listing) do |f| %>
  <%= f.input :title %>
  <%= f.input :description %>
  <%= f.input :address_line_1 %>
  <%= f.input :address_line_2 %>
  <%= f.input :city %>
  <%= f.input :postcode %>
  <%= f.input :available %>
  <%= f.input :rate %>
  <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  <%= f.submit %>
<% end %>
